type Account {
  created: String!
  lastUpdated: String!
  accountname: String!
  email: String!
  enabled: Boolean!
  blocked: Boolean!
  roles: [Role]!
}

input AccountInput {
  accountname: String!
  email: String!
  password: String!
  confirmation: String!
  roles: [RoleInput]
}

extend type RootQuery {
  allAccounts: [Account]!
}

extend type RootMutation {
  newAccount(accountInput: AccountInput!): Account!
}
