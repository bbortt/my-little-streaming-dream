{
  "private": true,
  "name": "next-js-web-ui",
  "description": "Static web application build on Next.js.",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "build": "next build",
    "export": "next export",
    "fullbuild": "npm run build && npm run export",
    "dev": "cross-env NODE_ENV=development next",
    "preview": "ws -d out -p 3000 --static.extensions html -o",
    "lint:check": "eslint --print-config .eslintrc | eslint-config-prettier-check",
    "flow": "flow check",
    "flow:coverage": "flow coverage",
    "flow:start": "flow",
    "flow:status": "flow status",
    "flow:stop": "flow stop",
    "prettier": "prettier \"{app,pages,server,shared,test}/**/*.js\"",
    "prettier:check": "npm run prettier -- --check",
    "prettier:fix": "npm run prettier -- --write ",
    "coverage": "nyc npm run test",
    "coverage:report": "npm run coverage && npm run coverage:upload",
    "coverage:upload": "codecov",
    "test": "cross-env NODE_ENV=test mocha",
    "test:watch": "npm run test -- --watch",
    "upgrade-interactive": "npm-check --update"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/bbortt/qdrakeboo.git"
  },
  "bugs": {
    "url": "https://github.com/bbortt/qdrakeboo/issues"
  },
  "homepage": "https://github.com/bbortt/qdrakeboo#readme",
  "dependencies": {
    "@zeit/next-sass": "^1.0.1",
    "apollo-cache-inmemory": "^1.6.3",
    "apollo-client": "^2.6.4",
    "apollo-link-http": "^1.5.16",
    "axios": "^0.19.0",
    "css-modules-require-hook": "^4.2.3",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.15.1",
    "foundation-sites": "^6.5.3",
    "graphql": "^14.5.8",
    "graphql-tag": "^2.10.1",
    "jquery": "^3.5.0",
    "lodash": "^4.17.19",
    "motion-ui": "^2.0.3",
    "next": "^9.3.2",
    "node-sass": "^4.13.0",
    "node-sass-package-importer": "^5.3.2",
    "react": "^16.11.0",
    "react-dom": "^16.11.0",
    "react-redux": "^7.1.1",
    "redux": "^4.0.4",
    "redux-devtools-extension": "^2.13.8",
    "redux-saga": "^1.1.1",
    "use-auth0-hooks": "^0.6.0",
    "uuid": "^3.3.3",
    "webpack": "^4.41.2"
  },
  "devDependencies": {
    "@babel/core": "^7.6.4",
    "@babel/plugin-proposal-export-default-from": "^7.5.2",
    "@babel/preset-flow": "^7.0.0",
    "@babel/register": "^7.6.2",
    "@istanbuljs/nyc-config-babel": "^2.1.1",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.6",
    "babel-plugin-istanbul": "^5.2.0",
    "chai": "^4.2.0",
    "codecov": "^3.6.1",
    "cross-env": "^6.0.3",
    "eslint": "^6.6.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-prettier": "^6.5.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-react": "^7.16.0",
    "flow-bin": "^0.110.1",
    "local-web-server": "^3.0.7",
    "mocha": "^6.2.2",
    "npm-check": "^5.9.0",
    "nyc": "^14.1.1",
    "prettier": "^1.18.2"
  }
